int main() {
    int num1,num2;
    
    printf ("Enter num1: ");
    scanf ("%d", &num1);
    
    printf ("Enter num2:");
    scanf ("%d", &num2);
    
    if (num1>num2) {
        printf ("num1: %d greater", num1);
    }
    else if (num1<num2) {
        printf ("num2: %d greater", num2);
    }
    
    else {
        printf ("Both are equal");
    }
}
