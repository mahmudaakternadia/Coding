#include <stdio.h>
int main()
{
    int n, n1=0, n2=1, s;
    
    printf ("How many fibonacci numbers you want: ");
    scanf ("%d", &n);
    
    printf ("%d %d ", n1, n2);
    
    for (int i=2; i<n; i++){
        s=n1+n2;
        n1=n2;
        n2=s;
        
        printf ("%d ", n2);
    }
}
